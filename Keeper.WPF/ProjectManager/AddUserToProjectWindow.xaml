<Window x:Class="Keeper.WPF.ProjectManager.AddUserToProjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keeper.WPF.ProjectManager"
        mc:Ignorable="d"
        Title="Add User To Project" Height="450" Width="270.27">
    <Grid>
        <DataGrid x:Name="UserList" HorizontalAlignment="Left" Height="304" Margin="10,66,0,0" VerticalAlignment="Top" Width="242"/>
        <TextBox x:Name="SearchTxtBox" HorizontalAlignment="Left" Height="23" Margin="111,38,0,0" TextWrapping="Wrap" TextChanged="SearchTxtBox_TextChanged" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="FilterLbl" Content="Filter" HorizontalAlignment="Left" Margin="33,35,0,0" VerticalAlignment="Top" Width="73"/>
        <Button x:Name="AddToProjectButton" Content="Add To Project" HorizontalAlignment="Left" Margin="60,375,0,0" VerticalAlignment="Top" Width="126" Height="40" Click="AddToProjectButton_Click"/>

    </Grid>
</Window>
